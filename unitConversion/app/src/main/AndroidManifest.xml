<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamdevelopment.b.unitconversion" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:icon="@mipmap/ic_launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".TempActivity"
            android:label="@string/temp_Title"
            android:icon="@mipmap/ic_launcher"/>
        <activity
            android:name=".LengthActivity"
            android:label="@string/length_Title"
            android:icon="@mipmap/ic_launcher"/>
        <activity
            android:name=".WeightActivity"
            android:label="@string/weight_Title"
            android:icon="@mipmap/ic_launcher"/>
        <activity
            android:name=".VolumeActivity"
            android:label="@string/volume_Title"
            android:icon="@mipmap/ic_launcher"/>
        <activity
            android:name=".TimeActivity"
            android:label="@string/time_Title"
            android:icon="@mipmap/ic_launcher"/>
    </application>

</manifest>
